{
  "name": "nextjs-app",
  "version": "0.0.1",
  "private": true,
  "description": "A nextjs app setup by jordy",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "prettier -c **/*.{js,ts,tsx}",
    "lint:fix": "prettier --write **/*.{js,ts,tsx}",
    "start": "next start",
    "test": "jest",
    "test:all": "jest && cypress run",
    "test:ci": "jest --ci",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:fix"
    }
  },
  "keywords": [
    "nextjs",
    "typescript",
    "tailwindcss",
    "jest",
    "testing-library/react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/browser": "^5.30.0",
    "@sentry/integrations": "^5.30.0",
    "@sentry/node": "^5.30.0",
    "@sentry/webpack-plugin": "^1.14.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "next": "^10.0.5",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "recoil": "^0.1.2",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "autoprefixer": "^10.2.1",
    "babel-jest": "^26.6.3",
    "cypress": "^6.2.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-next-dynamic": "^1.0.1",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "postcss": "^8.1.2"
  }
}
